#include<stdio.h>
#include<string.h>
void main()
 {
    char pass[20];
    printf("enter your password");
    scanf("%s",pass);
    int len= strlen (pass);
    int l=0,d=0,u=0,s=0;
    if(len>7)
    {
        for( int i=0;i<len;i++)
        {
          if(isupper (pass[i]))
                u++;
          else if(islower (pass[i]))
                l++;
          else if(isdigit (pass[i]))
                d++;
        else
           s++;
        }
        if(u>0&&l>0&&d>0&&s>0)
        {
            printf("password is strong");
        }
        else
         {
             printf("weak");
         }
    }
    else
        printf("weak due to less character");
}


OUTPUT:
enter your passwordtrivENICHennu108wAsteFeLL0w
password is strong
